{
  "name": "curso_webpack",
  "version": "1.0.0",
  "description": "Curso de Webpack 4",
  "main": "index.js",
  "scripts": {
    "build": "webpack --entry ./index.js --output ./bundle.js --mode development",
    "build:local": "webpack --mode production",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:multi": "webpack --config ./multi-entry-points/webpack.config.js",
    "build:css": "webpack --config ./CSS-Styles-Loaders/webpack.config.js",
    "build:plugins": "webpack --config ./Plugins/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./Dev-Server/webpack.config.js",
    "build:babel": "webpack-dev-server --config ./babel/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@pablomdd",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "babel-loader": "8.0.6",
    "css-loader": "3.4.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "style-loader": "1.1.2",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  }
}
